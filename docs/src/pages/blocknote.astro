	'/3d_models/openscad/blocknote/blocknote.stl',
	'/3d_models/openscad/blocknote/cover.stl',
	'/3d_models/openscad/blocknote/cover_back.stl',
	'/3d_models/openscad/blocknote/liner.stl',
